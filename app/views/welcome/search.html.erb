<% @results.each do |result| %>
<p><%= link_to "#{result.name}", restaurant_path(result) %></p>
  <% if result.latitude != nil %>
    <p><%= result.distance_from([session[:latitude], session[:longitude]]).round(2) %> miles away.</p>
  <%end%>
<%end%>
<div class="apple_pagination">
<center><%= will_paginate @results %></center>
</div>